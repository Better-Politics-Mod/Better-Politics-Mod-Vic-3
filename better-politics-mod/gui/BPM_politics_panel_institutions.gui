								expand = {}

								#current progress / cancel
								widget = {
								
									flowcontainer = {
										resizeparent = yes
										visible = "[IsValid( Institution.StopSizeChange )]"
										parentanchor = vcenter
										direction = vertical
										spacing = 0
										margin = { 0 10 }
										
										alpha = 0
										position = { 100 0 }
										
										state = {
											name = _show
											duration = 0.3
											alpha = 1
											position_x = 0
											using = Animation_Curve_Default
										}
										state = {
											name = _hide
											duration = 0.3
											alpha = 0
											position_x = 100
											using = Animation_Curve_Default
										}
										
