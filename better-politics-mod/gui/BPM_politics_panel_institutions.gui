						visible = "[GetVariableSystem.Exists('bpm_show_all_ministries')]"
						parentanchor = center
						expand = {}
						icon = {
							texture = "gfx/interface/buttons/button_icons/minus.dds"
							size = { 23 23 }
							# parentanchor = left
						}
						textbox = {
							autoresize = yes
							text = "bpm_button_hide_all"
							# parentanchor = center
							align = nobaseline
							margin_right = 5
						}
						expand = {}
