									# have to have this size here to avoid the text pushing out other stuff ( it is adapted for lowest resolution )
									# https://beta.paradoxplaza.com/browse/PSGE-1949
									minimumsize = { 375 -1 }
									maximumsize = { 375 -1 }
									multiline = yes
									margin = { 0 10 }
